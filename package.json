{
  "name": "rollup-plugin-tsc-generator",
  "version": "0.0.3",
  "description": "Rollup plugin to generate typescript declarations using tsc native",
  "scripts": {
    "types": "npx tsc --emitDeclarationOnly",
    "build": "node build.js && npm run types"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "exports": {
    "import": "./dist/index.mjs",
    "default": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juandl/rollup-plugin-tsc-generator.git"
  },
  "keywords": [
    "rollup",
    "plugin",
    "typescript",
    "types",
    "interfaces",
    "javascript"
  ],
  "author": "juandl",
  "license": "MIT",
  "contributors": [
    "Juan David"
  ],
  "bugs": {
    "url": "https://github.com/juandl/rollup-plugin-tsc-generator/issues"
  },
  "homepage": "https://github.com/juandl/rollup-plugin-tsc-generator#readme",
  "peerDependencies": {
    "rollup": "^3.0.0 || ^4.0.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.4.0",
    "@swc/core": "^1.7.0",
    "@types/node": "^20.14.11",
    "typescript": "^5.3.3"
  }
}
